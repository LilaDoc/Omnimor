{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/url",
            "dest": "index.js"
        },
        {
            "src": "/article",
            "dest": "index.js"
        },
        {
            "src": "/articles",
            "dest": "index.js"
        },
        {
            "src": "/add",
            "dest": "index.js"
        },
        {
            "src": "/delete",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
} 